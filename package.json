    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.37.3",
    "svelte-check": "^4.3.1",
    "svelte-language-server": "^0.17.18",
    "typescript": "^5.9.2",
